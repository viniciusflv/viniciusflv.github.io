{
  "name": "personal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prd": "npm run build && npm run preview",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "svj": "svj -i src/assets/svg -d src/assets/svg -r",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,cjs,mjs}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@astrojs/mdx": "^0.9.0",
    "@astrojs/react": "^1.0.0",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^1.0.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "astro": "^1.0.5",
    "astro-compress": "^0.0.18",
    "astro-robots-txt": "^0.3.7",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svj": "^2.2.3"
  },
  "dependencies": {
    "@svjson/react": "^0.0.3"
  }
