{
  "name": "personal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prd": "npm run build && npm run preview",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
